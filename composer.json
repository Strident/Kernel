{
    "name": "strident/kernel",
    "type": "library",
    "description": "Kernel component designed to bootstrap Trident applications.",
    "keywords": [ "kernel", "component", "trident" ],
    "license": "MIT",
    "homepage": "https://github.com/Strident/Kernel",
    "authors": [
        {
            "name": "Elliot Wright",
            "email": "elliot@elliotwright.co"
        }
    ],
    "require": {
        "php": ">=5.4",
        "strident/container": "~2.0",
        "symfony/event-dispatcher": "~2.7",
        "symfony/http-foundation": "~2.7"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.5",
        "codeclimate/php-test-reporter": "~0.1"
    },
    "autoload": {
        "psr-4": { "Strident\\Kernel\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": { "Strident\\Kernel\\Tests\\": "tests/" }
    },
    "minimum-stability": "dev"
}
